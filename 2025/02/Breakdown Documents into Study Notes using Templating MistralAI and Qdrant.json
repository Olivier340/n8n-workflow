{"createdAt":"2025-02-24T08:57:25.122Z","updatedAt":"2025-02-24T10:10:14.326Z","id":"PYu9Zm6Y86gbOmvZ","name":"Breakdown Documents into Study Notes using Templating MistralAI and Qdrant","active":false,"nodes":[{"parameters":{"triggerOn":"folder","path":"/home/node/storynotes/context","events":["add"],"options":{"followSymlinks":true,"usePolling":true}},"id":"a73c35cf-f7a8-49d1-a050-c5a063e0d685","name":"Local File Trigger","type":"n8n-nodes-base.localFileTrigger","position":[-2780,120],"typeVersion":1},{"parameters":{"jsonMode":"expressionData","jsonData":"={{ $json.data }}","options":{"metadata":{"metadataValues":[{"name":"project","value":"={{ $('Settings').item.json.project }}"},{"name":"filename","value":"={{ $('Settings').item.json.filename }}"}]}}},"id":"26712cb6-f609-42ff-8aae-f223a17cbea5","name":"Default Data Loader","type":"@n8n/n8n-nodes-langchain.documentDefaultDataLoader","position":[-1620,580],"typeVersion":1},{"parameters":{"chunkSize":2000,"options":{}},"id":"dc4d05f4-8c09-4bfc-9be8-541668be0c35","name":"Recursive Character Text Splitter","type":"@n8n/n8n-nodes-langchain.textSplitterRecursiveCharacterTextSplitter","position":[-1620,720],"typeVersion":1},{"parameters":{"options":{}},"id":"0b058f31-8dcb-4c84-a13d-0ef48e8fb0bd","name":"Embeddings Mistral Cloud","type":"@n8n/n8n-nodes-langchain.embeddingsMistralCloud","position":[-1740,580],"typeVersion":1,"credentials":{"mistralCloudApi":{"id":"NBeSmYZTNG8XbHEk","name":"Mistral Cloud account"}}},{"parameters":{"model":"open-mixtral-8x7b","options":{}},"id":"09ae6aab-6b93-4f31-a5dc-4cbb09d07f73","name":"Mistral Cloud Chat Model","type":"@n8n/n8n-nodes-langchain.lmChatMistralCloud","position":[-260,700],"typeVersion":1,"credentials":{"mistralCloudApi":{"id":"NBeSmYZTNG8XbHEk","name":"Mistral Cloud account"}}},{"parameters":{"model":"open-mixtral-8x7b","options":{}},"id":"d9850c52-0bf1-45dd-b728-6dfaf0c787aa","name":"Mistral Cloud Chat Model1","type":"@n8n/n8n-nodes-langchain.lmChatMistralCloud","position